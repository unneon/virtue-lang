func print_int(n int) int
    is_negative = n < 0
    if is_negative
        n = -n
    s = new string
    s.pointer = alloc(21, i8)
    s.length = 20
    s[20] = 0
    i = 0
    while n > 0
        s[s.length - i - 1] = 48 + n % 10
        i++
        n = n / 10
    if i == 0
        s[s.length - i - 1] = 48
        i++
    if is_negative
        s[s.length - i - 1] = 45
        i++
    s.pointer = s.pointer + s.length - i
    s.length = i
    print "{s}"
    return 0

print_int(9223372036854775807)
print_int(1234)
print_int(7)
print_int(0)
print_int(-3)
print_int(-432)
print_int(-9223372036854775807)

# 9223372036854775807
# 1234
# 7
# 0
# -3
# -432
# -9223372036854775807
